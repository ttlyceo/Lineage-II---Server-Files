+				}
+			}
+			contDrop++;
+			if(contDrop>Hasta){
+				haveNext=true;
+			}			
+		}
+		
+		if(contador>0 && contador<3){
+			for(int i=contador;i<3;i++){
+				DropData += GrillaDropBox.replace("%IDIMG%", "").replace("%NOMBRE%", "").replace("%DROPMIN%", "").replace("%DROPMAX%", "").replace("%PORCENTAJE%", "").replace("%TIPODROP%", "");
+			}
